extends layout 

block content 
    table 
        tr 
            td 
                fieldset Registration
                    form(action="/user/registration", method="post") 
                    p Login*
                        input(type="text", name="login", required)
                    p password*
                        input(type="password", name="password", required)
                    p
                        input(type="submit", value="Sign up")